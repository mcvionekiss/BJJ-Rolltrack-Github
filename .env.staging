# Staging Environment Configuration

# Frontend Configuration
REACT_APP_API_URL=https://staging.your-domain.com/api

# Backend Configuration
PIPELINE=staging
DJANGO_SECRET_KEY=your_staging_secret_key
ALLOWED_HOSTS=staging.your-domain.com
CORS_ALLOWED_ORIGINS=https://staging.your-domain.com

# Database Configuration
DB_NAME=bjj_rolltrack_staging
DB_USER=bjj_db_user
DB_PASSWORD=your_staging_db_password
DB_HOST=your-staging-db-host
DB_PORT=3306

# Disable HTTPS requirements for local testing if needed
# BYPASS_HTTPS=true

# Nginx Settings
DOMAIN_NAME=staging.your-domain.com
USE_HTTPS=true